<header class="continuum-header mw-header">
	<div class="continuum-header-start">
		<nav class="continuum-main-menu-landmark" aria-label="{{msg-continuum-site-nav-label}}">
			{{>MainMenuDropdown}}
		</nav>
		{{#data-logos}}{{>Logo}}{{/data-logos}}
	</div>
	<div class="subtheme-dropdown">
		<label for="subtheme-select">Theme:</label>
		<select id="subtheme-select">
			<option value="dark">🌑 Dark</option>
			<option value="medium">🌗 Medium</option>
			<option value="light">🌞 Light</option>
		</select>
	</div>
	<div class="continuum-header-end">
		{{#data-search-box}}{{>SearchBox}}{{/data-search-box}}
		{{#data-continuum-user-links}}{{>UserLinks}}{{/data-continuum-user-links}}
	</div>
</header>
